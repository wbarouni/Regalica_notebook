{
  "version": 2,
  "name": "regalica-notebook",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/angular",
      "config": {
        "distDir": "frontend/dist/regalica-frontend"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

